  "author": "Connor Peet <connor@peet.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/vscode-pwa-analyzer/issues"
  },
  "homepage": "https://github.com/microsoft/vscode-pwa-analyzer#readme",
  "devDependencies": {
    "@types/lodash-es": "^4.17.3",
    "@types/pako": "^1.0.1",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "bootstrap": "^4.5.0",
    "css-loader": "^3.6.0",
    "html-webpack-plugin": "^4.3.0",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-data-grid": "^7.0.0-canary.16",
    "react-dom": "^16.13.1",
    "react-json-view": "^1.19.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.9.5",
    "use-debounce": "^3.4.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "avoid",
    "printWidth": 100,
    "tabWidth": 2
  },
  "dependencies": {
    "lodash-es": "^4.17.15",
    "pako": "^1.0.11"
